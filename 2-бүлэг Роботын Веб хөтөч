  <div class="calculator">
    <h1>Батарейны Хэмжээ Тооцоолох</h1>
    <div class="input-section">
        <label for="devicePower">Хэрэглэгдэх Чадал (Ватт):</label>
        <input type="number" id="devicePower" placeholder="Чадал Оруулах">
    </div>

    <div class="input-section">
        <label for="operatingTime">Ашиглалтын Хугацаа (Цаг):</label>
        <input type="number" id="operatingTime" placeholder="Хугацаа Оруул">
    </div>

    <button onclick="calculateBatterySize()">Батарейны Чадал Тооцоолох</button>

    <div class="result" id="result"></div>

    <script>
        function calculateBatterySize() {
    const devicePower = parseFloat(document.getElementById('devicePower').value);
    const operatingTime = parseFloat(document.getElementById('operatingTime').value);

    if (isNaN(devicePower) || isNaN(operatingTime)) {
        document.getElementById('result').innerHTML = '<p>Please enter valid numbers.</p>';
        return;
    }

    const batterySize = devicePower * operatingTime;
    document.getElementById('result').innerHTML = `<p>Шаардлагатай Батарей Хэмжээ: ${batterySize} Ватт/Цаг</p>`;
}
    </script>
</div>

