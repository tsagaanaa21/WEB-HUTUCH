<body>

<div class="calculator">
  <h2>Хоёр Дугуйтай Роботын Кинематик Тооцоолох</h2>

  <label for="initialX">Анхны X Байрлал (м):</label>
  <input type="number" id="initialX" placeholder="Анхны X Байрлал Оруул">

  <label for="initialY">Анхны Y Байрлал (м):</label>
  <input type="number" id="initialY" placeholder="Анхны Y Байрлал Оруул">

  <label for="initialTheta">Анхны чиглэл (рад):</label>
  <input type="number" id="initialTheta" placeholder="Анхны чиглэл Оруул">

  <label for="leftVelocity">Зүүн Дугуйны Хурд (м/с):</label>
  <input type="number" id="leftVelocity" placeholder="Зүүн Дугуйны Хурд Оруул">

  <label for="rightVelocity">Баруун Дугуйны Хурд (м/с):</label>
  <input type="number" id="rightVelocity" placeholder="Баруун Дугуйны Хурд Оруул">

  <label for="time">Хугацаа (с):</label>
  <input type="number" id="time" placeholder="Хугацаа Оруул">

  <button onclick="calculatePosition()">Байршил тооцоолох</button>

  <label for="result">Хариу:</label>
  <p id="result"></p>

  <script>
      function calculatePosition() {
          const initialX = parseFloat(document.getElementById('initialX').value);
          const initialY = parseFloat(document.getElementById('initialY').value);
          const initialTheta = parseFloat(document.getElementById('initialTheta').value);
          const leftVelocity = parseFloat(document.getElementById('leftVelocity').value);
          const rightVelocity = parseFloat(document.getElementById('rightVelocity').value);
          const time = parseFloat(document.getElementById('time').value);

          // Kinematic equations for differential drive
          const v = (leftVelocity + rightVelocity) / 2;
          const omega = (rightVelocity - leftVelocity) / 2;

          // Update position based on kinematic equations
          const deltaX = v * Math.cos(initialTheta) * time;   
          const deltaY = v * Math.sin(initialTheta) * time;
          const deltaTheta = omega * time;

          const newX = initialX + deltaX;
          const newY = initialY + deltaY;
          const newTheta = initialTheta + deltaTheta;

          document.getElementById('result').innerText = `Шинэ X: ${newX.toFixed(2)} м, Шинэ Y: ${newY.toFixed(2)} м, Шинэ Өнцөг: ${newTheta.toFixed(2)} рад`;
      }
  </script>
</div>
