<div class="calculator">
<h2>Sensor Calculation for Education</h2>

<label for="sensorType">Select Sensor Type:</label>
<select id="sensorType">
    <option value="ultrasonic">Ultrasonic Sensor</option>
    <option value="infrared">Infrared Sensor</option>
    <option value="accelerometer">Accelerometer</option>
    <!-- Add more sensor options as needed -->
</select>

<label for="sensorValue">Enter Sensor Value:</label>
<input type="number" id="sensorValue" placeholder="Enter sensor value">

<button onclick="calculate()">Calculate</button>

<p id="result"></p>

<script>
    function calculate() {
        const sensorType = document.getElementById('sensorType').value;
        const sensorValue = parseFloat(document.getElementById('sensorValue').value);
        let result = '';

        switch (sensorType) {
            case 'ultrasonic':
                result = `Distance measured by Ultrasonic Sensor: ${sensorValue} cm`;
                break;
            case 'infrared':
                result = `Infrared Sensor value: ${sensorValue}`;
                break;
            case 'accelerometer':
                result = `Acceleration measured by Accelerometer: ${sensorValue} m/s²`;
                break;
            // Add more cases for additional sensor types

            default:
                result = 'Invalid sensor type';
        }

        document.getElementById('result').textContent = result;
    }
</script>
</div>

